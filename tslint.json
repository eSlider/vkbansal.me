{
    "rules": {
        "await-promise": true,
        "array-type": true,
        "encoding": true,
        "typedef-whitespace": [
			true,
			{
			"call-signature": "onespace",
			"index-signature": "onespace",
			"parameter": "onespace",
			"property-declaration": "onespace",
			"variable-declaration": "onespace"
			},
			{
			"call-signature": "onespace",
			"index-signature": "onespace",
			"parameter": "onespace",
			"property-declaration": "onespace",
			"variable-declaration": "onespace"
			}
        ],
        "forin": true,
		"no-arg": true,
		"no-conditional-assignment": true,
		"no-console": true,
		"no-debugger": true,
		"no-duplicate-super": true,
		"no-duplicate-variable": true,
		"no-empty": true,
		"no-eval": true
    }
}
